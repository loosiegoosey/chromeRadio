{
  "name": "chromeRadio",
  "version": "1.0",
  "description": "Plays Mp3 Files from a Page",
  "background_page" : "background.html",
    "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/match_content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.html"
  },
  "options_page": "my-library.html"
}
